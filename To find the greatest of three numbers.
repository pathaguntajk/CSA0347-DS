#include<stdio.h>
#include<conio.h>
int main()
{
int a,b,c;
printf("enter a value:");
scanf("%d",&a);
printf("enter b value:");
scanf("%d",&b);
printf("enter c value:");
scanf("%d",&c);
if(a>b and a>c){
printf("a is greater");
}
else if(b>c){
printf("b is greater");
}
else{
printf("c is greater");
}
getch();
return 0;
}
